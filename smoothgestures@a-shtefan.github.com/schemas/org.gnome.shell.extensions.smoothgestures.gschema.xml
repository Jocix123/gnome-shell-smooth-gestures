<schemalist gettext-domain="smoothgestures">
    <enum id="org.gnome.shell.extensions.smoothgestures.actions">
        <value value="0" nick="overview"/>
        <value value="1" nick="cycle-apps"/>
        <value value="2" nick="app-drawer"/>
        <value value="3" nick="switch-workspace"/>
        <value value="4" nick="show-desktop"/>
        <value value="5" nick="nothing"/>
        <value value="6" nick="next-workspace"/>
        <value value="7" nick="previous-workspace"/>
        <value value="8" nick="go-forward"/>
        <value value="9" nick="go-back"/>
        <value value="10" nick="app-drawer-unanimated"/>
    </enum>
    <schema id="org.gnome.shell.extensions.smoothgestures" path="/org/gnome/shell/extensions/smoothgestures/">
        <key name="sensitivity" type="i">
            <default>50</default>
            <summary>Sensitivity of the gestures</summary>
            <description>100 is maximal sensitivity, 0 is minimal</description>
        </key>
    </schema>
</schemalist>
